{
  "name": "@qubeless/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "node scripts/resolve-failed-migrations.js && prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "test": "node --test --test-concurrency=1 test/**/*.test.js",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "jest --config jest.config.js --watch",
    "test:unit:coverage": "jest --config jest.config.js --coverage || true",
    "test:all": "pnpm test:unit && pnpm test",
    "test:all:coverage": "pnpm exec jest --config jest.config.js --coverage && pnpm test",
    "test:auth-local": "node --test test/auth-local.test.js",
    "test:oidc-mapping": "node --test test/oidc-mapping.test.js",
    "test:saml-mapping": "node --test test/saml-mapping.test.js",
    "test:logout": "node --test test/logout.test.js",
    "test:sso-optional": "node --test test/sso-optional.test.js",
    "test:security": "node --test test/security-headers.test.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/s3-request-presigner": "^3.670.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/swagger": "^8.11.1",
    "@fastify/swagger-ui": "^1.9.3",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-fastify": "^10.2.7",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^7.1.15",
    "@nestjs/throttler": "^6.5.0",
    "@node-saml/node-saml": "^5.1.0",
    "@prisma/client": "^5.7.1",
    "@qubeless/shared": "workspace:*",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.18.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cobertura-parse": "^1.0.5",
    "fastify": "^4.24.3",
    "handlebars": "^4.7.8",
    "jose": "^5.6.3",
    "lcov-parse": "^1.0.0",
    "ldapjs": "^3.0.7",
    "nestjs-throttler-storage-redis": "^0.5.1",
    "nodemailer": "^7.0.12",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.7",
    "@swc/core": "^1.15.7",
    "@swc/jest": "^0.2.39",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/lcov-parse": "^1.0.2",
    "@types/ldapjs": "^3.0.6",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^3.0.9",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.54.0",
    "ioredis": "^5.8.2",
    "jest": "^30.2.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
