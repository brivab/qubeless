{
  "name": "@qubeless/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.vue",
    "format": "prettier --write \"src/**/*.{ts,vue}\"",
    "typecheck": "vue-tsc --noEmit",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist",
    "cypress:open": "cypress open --config-file cypress.config.ts",
    "cypress:run": "cypress run --config-file cypress.config.ts",
    "cypress:demo": "cypress run --config-file cypress.config.ts --spec \"cypress/e2e/demo/*.demo.cy.ts\" --browser chrome --headless --no-runner-ui --config video=true,videoCompression=false,screenshotOnRunFailure=false,retries=0,viewportWidth=1920,viewportHeight=1080"
  },
  "dependencies": {
    "@qubeless/shared": "workspace:*",
    "chart.js": "^4.5.1",
    "pinia": "^2.1.7",
    "qrcode": "^1.5.3",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.23",
    "cypress": "^13.17.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tailwindcss": "3",
    "typescript": "^5.3.3",
    "vite": "^4.5.0",
    "vue-tsc": "^2.2.12"
  }
}
